<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">

	<param name="usuarioId" value="#{enfermeroCrear_custom.usuarioId}" />
	<param name="open" />
	<param name="avz" />
	<param name="order" />
	<param name="firstResult" />
	<param name="critB1" />
	<param name="critB2" />
	<param name="critB3" />
	<param name="critB4" />

	<navigation from-action="#{enfermeroCrear_custom.crear()}">
		<rule if-outcome="gotodetails">
			<redirect
				view-id="/modConfiguracion/management/gestionar_enfermeros/enfermero_details.xhtml">
				<param name="enfermeroId" value="#{enfermeroCrear_custom.enfermero.id}" />
				<param name="from" value="crear" />
				<param name="open" value="true" />
				<param name="avz" value="false" />
				<param name="order" value="enfermera.id desc" />
				<param name="firstResult" value="0" />
				<param name="critB1" value="" />
				<param name="critB2" value="" />
				<param name="critB3" value="" />
				<param name="critB4" value="" />
			</redirect>
			<end-conversation before-redirect="true" />
		</rule>
	</navigation>
	<navigation from-action="cancelar">
		<redirect
			view-id="/modConfiguracion/management/gestionar_enfermeros/enfermero_list.gehos">
			<param name="open" value="#{open}" />
			<param name="avz" value="#{avz}" />
			<param name="order" value="#{order}" />
			<param name="firstResult" value="#{firstResult}" />
			<param name="critB1" value="#{critB1}" />
			<param name="critB2" value="#{critB2}" />
			<param name="critB3" value="#{critB3}" />
			<param name="critB4" value="#{critB4}" />
		</redirect>
		<end-conversation before-redirect="true" />
	</navigation>
</page>
