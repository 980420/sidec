<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">

	<action execute="#{tipoEstablecimientoBuscarControlador.begin()}" />
	
	<!-- Create -->
	<navigation from-action="#{tipoEstablecimientoCrearControlador.begin()}" evaluate="#{tipoEstablecimientoCrearControlador.error}">	
		<rule if-outcome="0">	
			<redirect view-id="/modConfiguracion/management/gestionarTipoEstablecimiento/CrearTipoEstablecimiento.xhtml" />
		</rule>
	</navigation>
	
	<!-- View details -->
	<navigation from-action="#{tipoEstablecimientoVerDetallesControlador.setId(establecimiento.id)}" evaluate="#{tipoEstablecimientoVerDetallesControlador.error}">
		<rule if-outcome="0">
			<redirect view-id="/modConfiguracion/management/gestionarTipoEstablecimiento/VerDetallesTipoEstablecimiento.xhtml">
				<param name="from" value="listar" />
			</redirect>
		</rule>
	</navigation>
	
	<!-- Modify -->
	<navigation from-action="#{tipoEstablecimientoModificarControlador.setId(establecimiento.id)}" evaluate="#{tipoEstablecimientoModificarControlador.error}">
		<rule if-outcome="0">
			<redirect view-id="/modConfiguracion/management/gestionarTipoEstablecimiento/ModificarTipoEstablecimiento.xhtml" />
		</rule>
	</navigation>
</page>