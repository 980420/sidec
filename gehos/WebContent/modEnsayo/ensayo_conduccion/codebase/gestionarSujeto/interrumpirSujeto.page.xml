           xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">  
           
    <begin-conversation flush-mode="manual" join="true"/>
    
	<!-- <param name="idMS"/> -->
	<param name="idSujeto" value="#{interrumpirSujeto.idSujeto}"/>
	<action if="#{!interrumpirSujeto.fechaInterrupcionRequired}" execute="#{interrumpirSujeto.loadData()}"/>
	
	<navigation from-action="#{interrumpirSujeto.InterrumpirSujeto()}">
			<redirect view-id="/modEnsayo/ensayo_conduccion/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarSujeto/buscarSujeto.xhtml">
			</redirect>
	</navigation>
	
	<navigation from-action="cancelar">
		<end-conversation before-redirect="true"/>
           <redirect view-id="/modEnsayo/ensayo_conduccion/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarSujeto/buscarSujeto.xhtml" >
          		 <param name="idFuncionalidad" value="#{activeModule.activeModule.id}" />
   			 	 <param name="idancestors" value="#{activeModule.activeModule.id}a" />	           		
           </redirect>
	</navigation>
</page>