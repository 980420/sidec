<?xml version="1.0" encoding="UTF-8"?>  
<page    xmlns               ="http://jboss.com/products/seam/pages"  
         xmlns:xsi           ="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation  ="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">  
         
	<begin-conversation join="true" flush-mode="manual"/>
	<action execute="#{modificarCronogramaEspecifico.loadData()}"/>
	
	<param name="idSujeto" value="#{modificarCronogramaEspecifico.idSujeto}"/>
	<param name="idMomento" value="#{modificarCronogramaEspecifico.idmomento}"/>
	
	<param name="from"/>
	
	
	<navigation from-action="#{modificarCronogramaEspecifico.salir()}">
	  <rule if="#{not empty modificarCronogramaEspecifico.sujeto}">
		<end-conversation before-redirect="true"/>
           <redirect view-id="/modEnsayo/ensayo_conduccion/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarCronogramaEspecifico/buscarCronogramaEspecifico.xhtml" >	           		
           </redirect>
       </rule>
	</navigation>
	
	<navigation from-action="#{modificarCronogramaEspecifico.modificarCronogramaEspecifico(momento.fechaInicio)}">
		<end-conversation before-redirect="true"/>	
		<redirect view-id="/modEnsayo/ensayo_conduccion/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarCronogramaEspecifico/causaModificarCronogramaEspecifico.xhtml">
	  	<param name="idMomento" value="#{idMomento}"/>
	  	
	  	</redirect>
	</navigation>
	
 </page>
