<?xml version="1.0" encoding="UTF-8"?>  
<page    xmlns               ="http://jboss.com/products/seam/pages"  
         xmlns:xsi           ="http://www.w3.org/2001/XMLSchema-instance" 
           xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">  
           
  	<begin-conversation flush-mode="manual" join="true"/>    
	<action f="#{crearMSNoProgramado.inicioConversacion == false}" execute="#{crearMSNoProgramado.inicializarMSNoProgramado()}"/>
	<param name="idCronograma" value="#{crearMSNoProgramado.idCronograma}" />
	 
	
	<navigation from-action="#{crearMSNoProgramado.insertarMSNoProgramado()}">	
		<rule if-outcome="ok">
			 	<end-conversation before-redirect="true"/> 	
			<redirect view-id="/modEnsayo/ensayo_disenno/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarMS/buscarMSNoProgramados/buscarMSNoProgramados.gehos">
			<param name="idCronograma" value="#{crearMSNoProgramado.idCronograma}"/>
			
			</redirect>
		</rule>
	</navigation>
	<navigation from-action="cancelar">	
		
			<end-conversation before-redirect="true"/>  		
			<redirect view-id="/modEnsayo/ensayo_disenno/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarMS/buscarMSNoProgramados/buscarMSNoProgramados.gehos">
			
			<param name="idCronograma" value="#{crearMSNoProgramado.idCronograma}"/>
			</redirect>
		
	</navigation>
	
	
	
	
</page>