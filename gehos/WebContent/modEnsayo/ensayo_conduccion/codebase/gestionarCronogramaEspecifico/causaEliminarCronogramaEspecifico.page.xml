<?xml version="1.0" encoding="UTF-8"?>  
<page    xmlns               ="http://jboss.com/products/seam/pages"  
         xmlns:xsi           ="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation  ="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">  
         
	<begin-conversation join="true" flush-mode="manual"/>
	<action execute="#{eliminarCronogramaEspecifico.loadData()}"/>
	
	<param name="idSujeto" value="#{eliminarCronogramaEspecifico.idSujeto}"/>
	<param name="from"/>
	
	<navigation from-action="#{verCronogramaEspecifico.salir()}">
	  <rule if="#{not empty verCronogramaEspecifico.sujeto}">
		<end-conversation before-redirect="true"/>
           <redirect view-id="/modEnsayo/ensayo_conduccion/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarCronogramaEspecifico/buscarCronogramaEspecifico.xhtml" >
           	           		
           </redirect>
       </rule>
	</navigation>
	
	<navigation from-action="cancelar">
		<end-conversation before-redirect="true"/>	
		<redirect view-id="/modEnsayo/ensayo_conduccion/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarCronogramaEspecifico/buscarCronogramaEspecifico.xhtml">
	  	<param name="idSujeto" value="#{idSujeto}"/>
	  	</redirect>
	</navigation>
	
	<navigation from-action="#{eliminarCronogramaEspecifico.eliminarCronogramaEspecifico()}">
		<end-conversation before-redirect="true"/>	
		<redirect view-id="/modEnsayo/ensayo_conduccion/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarCronogramaEspecifico/buscarCronogramaEspecifico.xhtml">
	  	<param name="idSujeto" value="#{idSujeto}"/>
	  	</redirect>
	</navigation>
	
 </page>
