<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">

	<param name="ubicacionId" value="#{ubicacionModificarControlador.ubicacionId}" />
	<param name="state" />
	<param name="order" />
	<param name="firstResult" />
	<param name="critB1" />
	<param name="critB2" />
	<param name="critB3" />
	<param name="searchMode"/>

	<!-- create and go to details -->  
	<navigation from-action="#{ubicacionModificarControlador.modificar()}" evaluate="#{ubicacionModificarControlador.error}">
		<rule if-outcome="0">
			<redirect view-id="/modConfiguracion/management/gestionarUbicacion/VerDetallesUbicacion.xhtml">
				<param name="ubicacionId" value="#{ubicacionModificarControlador.ubicacion.id}" />
				<param name="state" value="#{state}" />
				<param name="order" value="#{order}" />				
				<param name="critB1" value="#{critB1}" />
				<param name="critB2" value="#{critB2}" />
				<param name="critB3" value="#{critB3}" />				
				<param name="searchMode" value="#{searchMode}" />
				<param name="firstResult" value="#{firstResult}" />
			</redirect>
			<end-conversation before-redirect="true" />
		</rule>
	</navigation>

	<!-- cancel and go to search -->  
	<navigation from-action="cancelar">
		<redirect view-id="/modConfiguracion/management/gestionarUbicacion/BuscarUbicacion.xhtml">
			<param name="state" value="#{state}" />
			<param name="order" value="#{order}" />				
			<param name="critB1" value="#{critB1}" />
			<param name="critB2" value="#{critB2}" />
			<param name="critB3" value="#{critB3}" />				
			<param name="searchMode" value="#{searchMode}" />
			<param name="firstResult" value="#{firstResult}" />
		</redirect>
		<end-conversation before-redirect="true" />		
	</navigation>
-</page>
