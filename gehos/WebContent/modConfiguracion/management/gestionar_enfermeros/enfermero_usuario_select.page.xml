<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">

	<param name="open" />
	<param name="avz" />
	<param name="order" />
	<param name="firstResult" />
	<param name="critB1" />
	<param name="critB2" />
	<param name="critB3" />
	<param name="critB4" />

	<action execute="#{enfermeroUsuarioSelect_custom.begin()}" />

	<navigation from-action="createEnfermero">
		<redirect
			view-id="/modConfiguracion/management/gestionar_enfermeros/enfermero_create.xhtml">
			<param name="usuarioId" value="#{enfermeroUsuarioSelect_custom.idUsuario}" />
			<param name="open" value="#{open}" />
			<param name="avz" value="#{avz}" />
			<param name="order" value="#{order}" />
			<param name="firstResult" value="#{firstResult}" />
			<param name="critB1" value="#{critB1}" />
			<param name="critB2" value="#{critB2}" />
			<param name="critB3" value="#{critB3}" />
			<param name="critB4" value="#{critB4}" />
		</redirect>
		<end-conversation before-redirect="true" />
	</navigation>

	<navigation from-action="cancelar">
		<redirect
			view-id="/modConfiguracion/management/gestionar_enfermeros/enfermero_list.gehos">
			<param name="open" value="#{open}" />
			<param name="avz" value="#{avz}" />
			<param name="order" value="#{order}" />
			<param name="firstResult" value="#{firstResult}" />
			<param name="critB1" value="#{critB1}" />
			<param name="critB2" value="#{critB2}" />
			<param name="critB3" value="#{critB3}" />
			<param name="critB4" value="#{critB4}" />
		</redirect>
		<end-conversation before-redirect="true" />
	</navigation>
</page>