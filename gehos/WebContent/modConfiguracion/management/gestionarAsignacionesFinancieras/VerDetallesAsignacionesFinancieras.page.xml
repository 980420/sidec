<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">

		
	<param name="from" value="#{asignacionFinancieraVerDetallesControlador.from}" />

	<navigation from-action="modificar">
		<rule>
			<redirect
				view-id="/modConfiguracion/management/gestionarAsignacionesFinancieras/ModificarAsignacionesFinancieras.xhtml">
				
				<param name="from" value="#{from}" />
			</redirect>
		</rule>
	</navigation>

	<navigation from-action="salir">
		<rule
			if="#{asignacionFinancieraVerDetallesControlador.from.equals('crear')}">
			<redirect
				view-id="/modConfiguracion/management/gestionarEntidadesSistema/CrearEntidadSistema.xhtml" />
		</rule>
		<rule
			if="#{asignacionFinancieraVerDetallesControlador.from.equals('modificar')}">
			<redirect
				view-id="/modConfiguracion/management/gestionarEntidadesSistema/ModificarEntidadSistema.xhtml" />
		</rule>
	</navigation>

	<navigation from-action="#{asignacionFinancieraVerDetallesControlador.eliminar()}">
		<rule
			if="#{asignacionFinancieraVerDetallesControlador.from.equals('crear')}">
			<redirect
				view-id="/modConfiguracion/management/gestionarEntidadesSistema/CrearEntidadSistema.xhtml" />
		</rule>
		<rule
			if="#{asignacionFinancieraVerDetallesControlador.from.equals('modificar')}">
			<redirect
				view-id="/modConfiguracion/management/gestionarEntidadesSistema/ModificarEntidadSistema.xhtml" />
		</rule>
	</navigation>

</page>
