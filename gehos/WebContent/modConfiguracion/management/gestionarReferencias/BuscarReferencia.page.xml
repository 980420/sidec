<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">
   
   <action execute = "#{referenciaBuscarControlador.begin()}"/>  
   
   <navigation from-action="#{seleccionarEntidades.setId(referenciaBuscarControlador.entidadOrigen.id)}" evaluate="#{seleccionarEntidades.error}">
	   	<rule if-outcome="0">
	   		<redirect view-id="/modConfiguracion/management/gestionarReferencias/SeleccionarEntidadesReferenciadas.xhtml" />
	   	</rule>
   </navigation>   
   
   <navigation from-action="#{seleccionarServicios.cargarDatos(referenciaBuscarControlador.entidadOrigen.id, referenciaBuscarControlador.entidadDestino.id)}" evaluate="#{seleccionarServicios.error}">
   	<rule if-outcome="0">
   		<redirect view-id="/modConfiguracion/management/gestionarReferencias/SeleccionarServicios.xhtml" />	
   	</rule>
   </navigation>  

</page>
