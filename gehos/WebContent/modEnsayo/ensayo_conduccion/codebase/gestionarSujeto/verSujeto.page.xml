<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">

	<begin-conversation join="true" flush-mode="manual" />
	<action execute="#{verSujeto.loadData()}" />
	<action execute="#{verCronogramaEspecifico.loadData()}" />

	<param name="idSujeto" value="#{verSujeto.idSujeto}" />
	<param name="idSujeto1" value="#{verCronogramaEspecifico.idSujeto}" />
	<param name="from" />


	<navigation from-action="#{verSujeto.salir()}">
		<rule if="#{not empty verSujeto.sujeto}">
			<end-conversation before-redirect="true" />
			<redirect
				view-id="/modEnsayo/ensayo_conduccion/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarSujeto/buscarSujeto.xhtml">
			</redirect>
		</rule>
	</navigation>
	<navigation from-action="export">

		<end-conversation />

	</navigation>
</page>
