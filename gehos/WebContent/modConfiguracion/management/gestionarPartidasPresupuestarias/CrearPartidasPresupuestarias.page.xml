<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">

	<action execute="#{partidaPresupuestariaCrearControlador.begin()}" />
	<param name="from" value="#{partidaPresupuestariaCrearControlador.from}"/>

	<navigation from-action="#{partidaPresupuestariaCrearControlador.crear()}">
		<rule if-outcome="gotodetails">
			<redirect
				view-id="/modConfiguracion/management/gestionarPartidasPresupuestarias/VerDetallesPartidasPresupuestarias.xhtml">
				<param name="from" value="#{from}" />
			</redirect>
		</rule>
	</navigation>

	<navigation from-action="#{partidaPresupuestariaCrearControlador.cancelar()}">
	<rule if="#{partidaPresupuestariaCrearControlador.from.equals('crear')}">
		<redirect
			view-id="/modConfiguracion/management/gestionarEntidadesSistema/CrearEntidadSistema.xhtml" />
			</rule>
			<rule if="#{partidaPresupuestariaCrearControlador.from.equals('modificar')}">
		<redirect
			view-id="/modConfiguracion/management/gestionarEntidadesSistema/ModificarEntidadSistema.xhtml" />
			</rule>
	</navigation>


</page>
