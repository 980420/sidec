<?xml version="1.0" encoding="UTF-8"?>  
<page    xmlns               ="http://jboss.com/products/seam/pages"  
         xmlns:xsi           ="http://www.w3.org/2001/XMLSchema-instance" 
           xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">  
           
  	<begin-conversation flush-mode="manual" join="true"/>    
  	

	 <action if="#{modificarConjuntoDatos.inicioConversacion==false}" execute="#{modificarConjuntoDatos.inicializarConjuntoDatos()}" />
   	<param name="idConjuntoDatos" value="#{modificarConjuntoDatos.idConjuntoDatos}" />				
	<param name="from"/>
	
	<navigation from-action="#{modificarConjuntoDatos.modificarConjuntoDatos()}">	
		<rule if-outcome="ok">
			<end-conversation before-redirect="true"/>  		
			<redirect view-id="/modEnsayo/ensayo_extraccion/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarConjuntoDatos/verDetallesConjuntoDatos/verDetallesConjuntoDatos.gehos">
				<param name="idConjuntoDatos" value="#{modificarConjuntoDatos.conjuntoDatos.id}" />				
				<param name="from" value="modificar" />
			</redirect>
		</rule>
		
	</navigation>
	<navigation from-action="cancelar">	
		
			<rule if="#{from == 'ver'}">
			<end-conversation before-redirect="true"/>  		
			<redirect view-id="/modEnsayo/ensayo_extraccion/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarConjuntoDatos/verDetallesConjuntoDatos/verDetallesConjuntoDatos.gehos">
				<param name="idConjuntoDatos" value="#{modificarConjuntoDatos.conjuntoDatos.id}" />				
				<param name="from" value="modificar" />
			</redirect>
		</rule>
		<rule if="#{from == 'buscar'}">
			<end-conversation before-redirect="true"/>  		
			<redirect view-id="/modEnsayo/ensayo_extraccion/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarConjuntoDatos/buscarConjuntoDatos/buscarConjuntoDatos.gehos">
				
			</redirect>
		</rule>
		
	</navigation>
	
	
	
	
</page>