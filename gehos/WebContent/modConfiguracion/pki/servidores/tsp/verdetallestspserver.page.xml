<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">

	<param name="id" value="#{verDetallesServidorTSPControlador.id}" />
	<param name="from" value="#{verDetallesServidorTSPControlador.from}" />

	<!-- go to modify -->
	<navigation
		from-action="#{modificarServidorTSPControlador.setId(verDetallesServidorTSPControlador.server.id)}" evaluate="#{modificarServidorTSPControlador.error}">
		<rule if-outcome="false">
			<redirect
				view-id="/modConfiguracion/pki/servidores/tsp/modificartspserver.xhtml">
				
			</redirect>
		</rule>
	</navigation>


	<navigation from-action="#{verDetallesServidorTSPControlador.eliminar()}">
		<rule if="#{!verDetallesServidorTSPControlador.error}">
		
			<redirect
				view-id="/modConfiguracion/pki/servidores/tsp/buscartspserver.xhtml" />
		</rule>
	</navigation>


	<navigation from-action="salir">
	
		<redirect
			view-id="/modConfiguracion/pki/servidores/tsp/buscartspserver.xhtml" />
	</navigation>

</page>