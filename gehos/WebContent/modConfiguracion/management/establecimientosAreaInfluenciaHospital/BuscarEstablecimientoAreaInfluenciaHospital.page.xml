	<navigation
		from-action="#{establecimientoAreaInfluenciaHospitalBuscarControlador.asignar()}">
		<rule if='#{from.equals("modificar")}'>
			<redirect
				view-id="/modConfiguracion/management/gestionarEntidadesSistema/ModificarEntidadSistema.xhtml" />
		</rule>
		<rule if='#{from.equals("crear")}'>
			<redirect
				view-id="/modConfiguracion/management/gestionarEntidadesSistema/CrearEntidadSistema.xhtml" />
		</rule>
	</navigation>

	<navigation
		from-action="#{establecimientoAreaInfluenciaHospitalBuscarControlador.setSalida(1)}">
