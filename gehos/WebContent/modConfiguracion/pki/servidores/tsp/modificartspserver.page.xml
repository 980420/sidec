<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">
	
	
	<!-- validation -->
		
	
	<navigation>		
		<rule if="#{modificarServidorTSPControlador.id} == null}">
			<redirect view-id="/modConfiguracion/pki/servidores/tsp/buscartspserver.xhtml" />
		</rule>				
	</navigation>

	<navigation from-action="#{modificarServidorTSPControlador.modificar()}" >
		<rule if="#{!modificarServidorTSPControlador.error}"> 
		<end-conversation before-redirect="true" />
			<redirect view-id="/modConfiguracion/pki/servidores/tsp/verdetallestspserver.xhtml">
				<param name="id" value="#{modificarServidorTSPControlador.server.id}" />
				<param name="from" value="modificatsp"/>
			</redirect>
			
		</rule>
	</navigation>
	
	<navigation from-action="seleccionarproxy">
		<redirect
			view-id="/modConfiguracion/pki/servidores/tsp/seleccionarproxymodificar.xhtml">
			
			</redirect>
	</navigation>

	<navigation from-action="cancelar">
	<end-conversation before-redirect="true" />		
			<redirect view-id="/modConfiguracion/pki/servidores/tsp/buscartspserver.xhtml" />
					
	</navigation>

</page>