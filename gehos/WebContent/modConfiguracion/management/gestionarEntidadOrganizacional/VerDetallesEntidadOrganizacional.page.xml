<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">

	<param name="id" value="#{entidadOrganizacionalVerDetallesControlador.id}" />
	<param name="from" value="#{entidadOrganizacionalVerDetallesControlador.from}" />
		
	<!-- validation -->	
	<navigation>
		<rule if="#{entidadOrganizacionalVerDetallesControlador.id == null or entidadOrganizacionalVerDetallesControlador.id == '' or entidadOrganizacionalVerDetallesControlador.errorLoadData == true}">
			<redirect view-id="/modConfiguracion/management/gestionarEntidadOrganizacional/BuscarEntidadOrganizacional.xhtml" />
		</rule>					
	</navigation>

	<!-- go to modify -->
	<navigation from-action="#{entidadOrganizacionalModificarControlador.cambId(entidadOrganizacionalVerDetallesControlador.entidadOrganizacional.id)}">
		<redirect view-id="/modConfiguracion/management/gestionarEntidadOrganizacional/ModificarEntidadOrganizacional.xhtml">
			<param name="id" value="#{entidadOrganizacionalVerDetallesControlador.entidadOrganizacional.id}" />
		</redirect>		
	</navigation>

	<!-- remove and go to view search -->
	<navigation from-action="#{entidadOrganizacionalVerDetallesControlador.eliminar()}" evaluate="#{entidadOrganizacionalVerDetallesControlador.error}">
		<rule if-outcome="0">
			<redirect view-id="/modConfiguracion/management/gestionarEntidadOrganizacional/BuscarEntidadOrganizacional.xhtml" />
		</rule>
	</navigation>

	<!-- go to view search -->
	<navigation from-action="salir">		
		<redirect view-id="/modConfiguracion/management/gestionarEntidadOrganizacional/BuscarEntidadOrganizacional.xhtml" />
	</navigation>	
</page>
