<?xml version="1.0" encoding="UTF-8"?>  
<page    xmlns               ="http://jboss.com/products/seam/pages"  
         xmlns:xsi           ="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation  ="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">  
         
	<begin-conversation join="true" flush-mode="manual"/>
	<action execute="#{crearCronogramaEspecifico.loadData()}"/>
	
	<param name="idSujeto" value="#{crearCronogramaEspecifico.idSujeto}"/>
	<param name="from"/>
	
	
	<navigation from-action="#{crearCronogramaEspecifico.salir()}">
	  <rule if="#{not empty crearCronogramaEspecifico.sujeto}">
		<end-conversation before-redirect="true"/>
           <redirect view-id="/modEnsayo/ensayo_conduccion/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarCronogramaEspecifico/buscarSujetoCronograma.xhtml" >
           	           		
           </redirect>
       </rule>
	</navigation>
	
	
	<navigation from-action="#{crearCronogramaEspecifico.Cronograma_Especifico()}">
		<end-conversation/>
		<redirect view-id="/modEnsayo/ensayo_conduccion/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarSujeto/verSujeto.xhtml">
		</redirect>	
	</navigation>
	
 </page>
