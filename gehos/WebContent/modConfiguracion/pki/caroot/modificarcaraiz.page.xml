<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">

	<begin-conversation flush-mode="manual" join="true" />
	<action execute="#{raFunctions.init()}" if="#{raFunctions.execute}" />
	<navigation from-action="#{raFunctions.modcaKeystore()}"
		evaluate="#{raFunctions.error}">

		<rule if-outcome="false">
			<end-conversation before-redirect="true" />
			<redirect view-id="/modConfiguracion/pki/caroot/verdetallescaraiz.xhtml"></redirect>

		</rule>
	</navigation>
	<navigation from-action="cancelar">
		<end-conversation before-redirect="true" />
		<redirect view-id="/modConfiguracion/pki/caroot/verdetallescaraiz.xhtml"></redirect>

	</navigation>
</page>