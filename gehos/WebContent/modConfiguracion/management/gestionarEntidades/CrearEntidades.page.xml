<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">
      
    <action execute="#{entidadCrearControlador.begin()}" />
	<param name="open" />
	<param name="firstResult" />
	<param name="order" />
	<param name="critB1" />   
  
  <navigation from-action="#{entidadCrearControlador.crear()}">		
		<rule if-outcome="gotodetails">				
			<redirect view-id="/modConfiguracion/management/gestionarEntidades/VerDetallesEntidades.xhtml">				
				<param name="entidadId"
					value="#{entidadCrearControlador.entidad.id}" />
				<param name="open" value="true" />
				<param name="firstResult" value="0" />
				<param name="order" value="entidad.id desc" />
				<param name="critB1" value="" />
				<param name="from" value="crear" />		   
			</redirect>
			<end-conversation before-redirect="true"/>	
		</rule>
	</navigation>
	
		<navigation from-action="cancelar">		
			<redirect
				view-id="/modConfiguracion/management/gestionarEntidades/BuscarEntidades.xhtml">
				<param name="open" value="#{open}" />
				<param name="firstResult" value="#{firstResult}" />
				<param name="order" value="#{order}" />
				<param name="critB1" value="#{critB1}" />				
			</redirect>
			<end-conversation before-redirect="true" />		
	</navigation>

</page>
