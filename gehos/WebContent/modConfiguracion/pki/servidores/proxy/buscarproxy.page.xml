<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">

	<action execute="#{buscarProxyControlador.begin()}"/>
	<!-- Create -->
	<navigation from-action="crearproxy">
		<redirect
			view-id="/modConfiguracion/pki/servidores/proxy/adicionarproxy.xhtml">
			<param name="from" value="listaproxy"/>
			</redirect>
	</navigation>
	<!-- Modify -->
	<navigation from-action="#{modificarProxyControlador.setId(proxy.id)}">
		<rule if="#{modificarProxyControlador.error == false}">
			<redirect view-id="/modConfiguracion/pki/servidores/proxy/modificarproxy.xhtml" />
		</rule>
	</navigation>
</page>