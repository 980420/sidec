<?xml version="1.0" encoding="UTF-8"?>  
<page    xmlns               ="http://jboss.com/products/seam/pages"  
         xmlns:xsi           ="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation  ="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">  
         
	<param name="idHojaCRD" value="#{visualizarHoja_Disenno.idcrd}" />
	<action execute="#{visualizarHoja_Disenno.initConversation()}"/>
	<param name="origen"/>
	
	<navigation from-action="cancelar">	
		<rule if="#{origen.equals('buscar')}">
			<end-conversation before-redirect="true"/>  		
			<redirect view-id="/modEnsayo/ensayo_disenno/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarCrd/modificarTransformacion.gehos">
			
			
			</redirect>
		</rule>
		<rule if="#{origen.equals('cargar')}">
			<end-conversation before-redirect="true"/>  		
			<redirect view-id="/modEnsayo/ensayo_disenno/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarCrd/modificarTransformacion.gehos">
			
			
			</redirect>
		</rule>
	</navigation>
 </page>
