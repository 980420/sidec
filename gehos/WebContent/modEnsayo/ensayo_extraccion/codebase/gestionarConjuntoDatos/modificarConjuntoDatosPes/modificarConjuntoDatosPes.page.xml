<?xml version="1.0" encoding="UTF-8"?>  
<page    xmlns               ="http://jboss.com/products/seam/pages"  
         xmlns:xsi           ="http://www.w3.org/2001/XMLSchema-instance" 
           xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">  
           
  	<begin-conversation flush-mode="manual" join="true"/>    
  	

	 <action if="#{modificarConjuntoDatosPes.inicioConversacion==false}" execute="#{modificarConjuntoDatosPes.inicializarConjuntoDatos()}" />
   	<param name="idConjuntoDatos" value="#{modificarConjuntoDatosPes.idConjuntoDatos}" />				
	<param name="from"/>
	
	<navigation from-action="#{modificarConjuntoDatosPes.modificarConjuntoDatos()}">	
		<rule if-outcome="ok">
			<end-conversation before-redirect="true"/>  		
			<redirect view-id="/modEnsayo/ensayo_extraccion/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarConjuntoDatos/verDetallesConjuntoDatosPes/verDetallesConjuntoDatosPes.gehos">
				<param name="idConjuntoDatos" value="#{modificarConjuntoDatosPes.conjuntoDatos.id}" />				
				<param name="from" value="modificar" />
			</redirect>
		</rule>
		
	</navigation>
	<navigation from-action="cancelar">	
		
			<rule if="#{from == 'ver'}">
			<end-conversation before-redirect="true"/>  		
			<redirect view-id="/modEnsayo/ensayo_extraccion/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarConjuntoDatos/verDetallesConjuntoDatosPes/verDetallesConjuntoDatosPes.gehos">
				<param name="idConjuntoDatos" value="#{modificarConjuntoDatosPes.conjuntoDatos.id}" />				
				<param name="from" value="modificar" />
			</redirect>
		</rule>
		<rule if="#{from == 'buscar'}">
			<end-conversation before-redirect="true"/>  		
			<redirect view-id="/modEnsayo/ensayo_extraccion/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarConjuntoDatos/buscarConjuntoDatos/buscarConjuntoDatos.gehos">
				
			</redirect>
		</rule>
		
	</navigation>
	
	
	
	
</page>