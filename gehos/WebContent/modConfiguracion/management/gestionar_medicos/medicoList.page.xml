<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">

	<action execute="#{medicoList_configuracionCustom.begin()}" />

	<param name="firstResult" value="#{medicoList_configuracionCustom.firstResult}" />
	<param name="order" value="#{medicoList_configuracionCustom.POrder}" />
	<param name="avz" value="#{medicoList_configuracionCustom.avanzada}" />
	<param name="open" value="#{medicoList_configuracionCustom.open}" />
	<param name="critB1" value="#{medicoList_configuracionCustom.matriculaMinisterio}" />
	<param name="critB2" value="#{medicoList_configuracionCustom.nombre}" />
	<param name="critB3" value="#{medicoList_configuracionCustom.apellido}" />
	<param name="critB4" value="#{medicoList_configuracionCustom.segundoapellido}" />
	<param name="critB5" value="#{medicoList_configuracionCustom.matriculaColegio}" />

	<navigation from-action="adicionar">
		<rule>
			<redirect
				view-id="/modConfiguracion/management/gestionar_medicos/medico_user_create.xhtml">
				<param name="firstResult" value="#{medicoList_configuracionCustom.firstResult}" />
				<param name="order" value="#{medicoList_configuracionCustom.order}" />
				<param name="avz" value="#{medicoList_configuracionCustom.avanzada}" />
				<param name="open" value="#{medicoList_configuracionCustom.open}" />
				<param name="critB1"
					value="#{parameters.encodec(medicoList_configuracionCustom.matriculaMinisterio)}" />
				<param name="critB2"
					value="#{parameters.encodec(medicoList_configuracionCustom.nombre)}" />
				<param name="critB3"
					value="#{parameters.encodec(medicoList_configuracionCustom.apellido)}" />
				<param name="critB4"
					value="#{parameters.encodec(medicoList_configuracionCustom.segundoapellido)}" />
				<param name="critB5"
					value="#{parameters.encodec(medicoList_configuracionCustom.matriculaColegio)}" />
			</redirect>
		</rule>
	</navigation>

	<navigation from-action="asociar">
		<rule>
			<redirect
				view-id="/modConfiguracion/management/gestionar_medicos/medico_select_user_create.xhtml">
				<param name="firstResult" value="#{medicoList_configuracionCustom.firstResult}" />
				<param name="order" value="#{medicoList_configuracionCustom.order}" />
				<param name="avz" value="#{medicoList_configuracionCustom.avanzada}" />
				<param name="open" value="#{medicoList_configuracionCustom.open}" />
				<param name="critB1"
					value="#{parameters.encodec(medicoList_configuracionCustom.matriculaMinisterio)}" />
				<param name="critB2"
					value="#{parameters.encodec(medicoList_configuracionCustom.nombre)}" />
				<param name="critB3"
					value="#{parameters.encodec(medicoList_configuracionCustom.apellido)}" />
				<param name="critB4"
					value="#{parameters.encodec(medicoList_configuracionCustom.segundoapellido)}" />
				<param name="critB5"
					value="#{parameters.encodec(medicoList_configuracionCustom.matriculaColegio)}" />
			</redirect>
		</rule>
	</navigation>


	<navigation from-action="#{usuarioList_configuracionCustom.eliminar()}">
		<rule>
			<redirect
				view-id="/modConfiguracion/management/gestionar_usuarios/userList.xhtml">
				<param name="firstResult" value="#{medicoList_configuracionCustom.firstResult}" />
				<param name="order" value="#{medicoList_configuracionCustom.order}" />
				<param name="avz" value="#{medicoList_configuracionCustom.avanzada}" />
				<param name="open" value="#{medicoList_configuracionCustom.open}" />
				<param name="critB1"
					value="#{parameters.encodec(medicoList_configuracionCustom.matriculaMinisterio)}" />
				<param name="critB2"
					value="#{parameters.encodec(medicoList_configuracionCustom.nombre)}" />
				<param name="critB3"
					value="#{parameters.encodec(medicoList_configuracionCustom.apellido)}" />
				<param name="critB4"
					value="#{parameters.encodec(medicoList_configuracionCustom.segundoapellido)}" />
				<param name="critB5"
					value="#{parameters.encodec(medicoList_configuracionCustom.matriculaColegio)}" />
			</redirect>
		</rule>
	</navigation>
	
	<navigation from-action="#{medicoList_configuracionCustom.seleccionarModificar()}">
		<rule if-outcome="good">
			<redirect
				view-id="/modConfiguracion/management/gestionar_medicos/modificar_medico.xhtml">
				<param name="userId" value="#{medicoList_configuracionCustom.idmedico}"/>				
				<param name="firstResult" value="#{medicoList_configuracionCustom.firstResult}" />
				<param name="order" value="#{medicoList_configuracionCustom.order}" />
				<param name="avz" value="#{medicoList_configuracionCustom.avanzada}" />
				<param name="open" value="#{medicoList_configuracionCustom.open}" />
				<param name="critB1"
					value="#{parameters.encodec(medicoList_configuracionCustom.matriculaMinisterio)}" />
				<param name="critB2"
					value="#{parameters.encodec(medicoList_configuracionCustom.nombre)}" />
				<param name="critB3"
					value="#{parameters.encodec(medicoList_configuracionCustom.apellido)}" />
				<param name="critB4"
					value="#{parameters.encodec(medicoList_configuracionCustom.segundoapellido)}" />
				<param name="critB5"
					value="#{parameters.encodec(medicoList_configuracionCustom.matriculaColegio)}" />
			</redirect>
		</rule>
	</navigation>
	
	<navigation from-action="#{medicoList_configuracionCustom.seleccionarVer()}">
		<rule if-outcome="good">
			<redirect
				view-id="/modConfiguracion/management/gestionar_medicos/medico_detail.xhtml">
				<param name="userId" value="#{medicoList_configuracionCustom.idmedico}"/>
				<param name="from" value="listar"/>
				<param name="firstResult" value="#{medicoList_configuracionCustom.firstResult}" />
				<param name="order" value="#{medicoList_configuracionCustom.order}" />
				<param name="avz" value="#{medicoList_configuracionCustom.avanzada}" />
				<param name="open" value="#{medicoList_configuracionCustom.open}" />
				<param name="critB1"
					value="#{parameters.encodec(medicoList_configuracionCustom.matriculaMinisterio)}" />
				<param name="critB2"
					value="#{parameters.encodec(medicoList_configuracionCustom.nombre)}" />
				<param name="critB3"
					value="#{parameters.encodec(medicoList_configuracionCustom.apellido)}" />
				<param name="critB4"
					value="#{parameters.encodec(medicoList_configuracionCustom.segundoapellido)}" />
				<param name="critB5"
					value="#{parameters.encodec(medicoList_configuracionCustom.matriculaColegio)}" />
			</redirect>
		</rule>
	</navigation>
	

</page>
