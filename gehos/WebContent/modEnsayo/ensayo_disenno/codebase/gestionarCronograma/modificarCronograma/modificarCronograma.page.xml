<?xml version="1.0" encoding="UTF-8"?>  
<page    xmlns               ="http://jboss.com/products/seam/pages"  
         xmlns:xsi           ="http://www.w3.org/2001/XMLSchema-instance" 
           xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">  
           
  	<begin-conversation flush-mode="manual" join="true"/>    
	
	<param name="idGrupoSujeto" value="#{modificarCronograma.idGrupoSujeto}" />
	<param name="idGrupoSujetoEtapas" value="#{etapasCustomList_ensayo.idGrupoSujetoEtapas}"/>   
   	<param name="firstResult" value="#{etapasCustomList_ensayo.firstResult}"/>
   	<action execute="#{modificarCronograma.inicializarCronogramaEtapas()}"/>
	<action execute="#{etapasCustomList_ensayo.refresh()}"/>
	
	<navigation from-action="#{modificarCronograma.modificarCronograma()}">	
		<rule if-outcome="ok">
			<end-conversation before-redirect="true"/>  		
			<redirect view-id="/modEnsayo/ensayo_disenno/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarCronograma/modificarCronograma/modificarCronograma.gehos">			
			</redirect>
		</rule>
	</navigation>
	
	<navigation from-action="cancelar">	
		
			<end-conversation before-redirect="true"/>  		
			<redirect view-id="/modEnsayo/ensayo_disenno/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarCronograma/listarGruposujetosCronograma/listarGruposujetosCronograma.gehos">
			
			</redirect>
		
	</navigation>
	
	
	
</page>