<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-name="modulo" match-schema="comun"></table-filter>
	<table-filter match-name="funcionalidad" match-schema="comun"></table-filter>
	<table-filter match-name="funcionalidad_in_role"
		match-schema="comun">
	</table-filter>
	<table-filter match-name="role" match-schema="seguridad"></table-filter>
</hibernate-reverse-engineering>