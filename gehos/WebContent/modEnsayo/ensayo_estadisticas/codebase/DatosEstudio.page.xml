<?xml version="1.0" encoding="UTF-8"?>  
<page    xmlns               ="http://jboss.com/products/seam/pages"  
         xmlns:xsi           ="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">  
 
	<action execute="#{crearSujeto.initConversation()}"/>
<!--	actionn="#{crearSujeto}"-->
	<navigation from-action="#{crearSujeto.persistir()}">
		<rule if="#{not crearSujeto.error}">
			<redirect view-id="/modEnsayo/ensayo_estadisticas/modules/#{activeModule.activeSubModulePhysicalName()}/ensayo_ensayo_estadisticas_3/DatosEstudio.xhtml">
				 <param name="idFuncionalidad" value="#{activeModule.activeModule.id}" />
	   			 <param name="idancestors" value="#{activeModule.activeModule.id}a" />
			</redirect>
		</rule>		
	</navigation>
	
	<navigation from-action="cancelar">
		<end-conversation/>
		<redirect view-id="/modEnsayo/ensayo_estadisticas/modules/#{activeModule.activeSubModulePhysicalName()}/ensayo_ensayo_estadisticas_3/DatosEstudio.xhtml">
			 <param name="idFuncionalidad" value="#{activeModule.activeModule.id}" />
   			 <param name="idancestors" value="#{activeModule.activeModule.id}a" />
		</redirect>	
	</navigation>
	
 </page>
