<?xml version="1.0" encoding="UTF-8"?>  
<page    xmlns               ="http://jboss.com/products/seam/pages"  
         xmlns:xsi           ="http://www.w3.org/2001/XMLSchema-instance" 
           xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">  
           
   <begin-conversation flush-mode="manual" join="true"/>
  	<param name="idNota" value="#{modificarNotaMonitoreo.idNota}" />
  	<param name="idSujeto" value="#{modificarNotaMonitoreo.idSujeto}"/>
  	<param name="idGrupo" value="#{modificarNotaMonitoreo.idGrupo}"/>	
	
	<action execute="#{modificarNotaMonitoreo.inicializarNota()}"/>
	
	<navigation from-action="#{modificarNotaMonitoreo.modificar()}">
    <end-conversation before-redirect="true"/>
		<redirect view-id="/modEnsayo/ensayo_conduccion/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarNotas/buscarNotas/listarNotasMonitoreo.xhtml">
			<param name="idSujeto" value="#{modificarNotaMonitoreo.idSujeto}"/>
			<param name="idGrupo" value="#{modificarNotaMonitoreo.idGrupo}"/>
		</redirect>
	</navigation>
	
	
	<navigation from-action="cancelar">
    <end-conversation before-redirect="true"/>
		<redirect view-id="/modEnsayo/ensayo_conduccion/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarNotas/buscarNotas/listarNotasMonitoreo.xhtml">
			<param name="idSujeto" value="#{modificarNotaMonitoreo.idSujeto}"/>
			<param name="idGrupo" value="#{modificarNotaMonitoreo.idGrupo}"/>
		</redirect>
	</navigation>
	
	
</page>