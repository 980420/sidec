<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">

	
	<param name="from" value="#{asignacionFinancieraModificarControlador.from}"/>

	<navigation from-action="#{asignacionFinancieraModificarControlador.modificar()}">
		<rule if-outcome="gotodetails">
			<redirect
				view-id="/modConfiguracion/management/gestionarAsignacionesFinancieras/VerDetallesAsignacionesFinancieras.xhtml">
				<param name="from" value="#{from}" />
			</redirect>
		</rule>
	</navigation>

	<navigation from-action="#{asignacionFinancieraModificarControlador.cancelar()}">
	<rule if="#{asignacionFinancieraModificarControlador.from.equals('crear')}">
		<redirect
			view-id="/modConfiguracion/management/gestionarEntidadesSistema/CrearEntidadSistema.xhtml" />
			</rule>
			<rule if="#{asignacionFinancieraModificarControlador.from.equals('modificar')}">
		<redirect
			view-id="/modConfiguracion/management/gestionarEntidadesSistema/ModificarEntidadSistema.xhtml" />
			</rule>
	</navigation>
</page>
