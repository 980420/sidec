<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">

	<param name="direccion" value="#{seleccionarEstudio.dirredirect}" />
	<param name="trazasD" value="#{seleccionarEstudio.trazasD}" />
	<param name="trazasC" value="#{seleccionarEstudio.trazasC}" />
	<param name="cMsXS" value="#{seleccionarEstudio.cantMS}" />
	<param name="conjDatos" value="#{seleccionarEstudio.cDatos}" />
	<action execute="#{seleccionarEstudio.buscarEstudio()}" />
	
	<navigation from-action="#{seguridadEstudio.PonerActivo(est)}"
		evaluate="#{seleccionarEstudio.direccionNula}">
		<rule if-outcome="1">
			<redirect view-id="/modCommons/modHome/home.gehos">
				<param name="idFuncionalidad" value="#{activeModule.activeModule.id}" />
				<param name="idancestors" value="#{activeModule.activeModule.id}a" />
				<param name="moduleName" value="#{activeModule.activeModuleName}" />
			</redirect>
		</rule>
		<rule if-outcome="0">
			<redirect view-id="#{seleccionarEstudio.dirredirect}"></redirect>
		</rule>
	</navigation>

</page>
