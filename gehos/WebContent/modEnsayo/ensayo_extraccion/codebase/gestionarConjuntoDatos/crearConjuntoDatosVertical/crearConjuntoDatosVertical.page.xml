<?xml version="1.0" encoding="UTF-8"?>  
<page    xmlns               ="http://jboss.com/products/seam/pages"  
         xmlns:xsi           ="http://www.w3.org/2001/XMLSchema-instance" 
           xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">  
           
  	<begin-conversation flush-mode="manual" join="true"/>    
  	

	 <action if="#{crearConjuntoDatosVertical.inicioConversacion==false}" execute="#{crearConjuntoDatosVertical.inicializarConjuntoDatos()}" />
   	
	
	<navigation from-action="#{crearConjuntoDatosVertical.insertarConjuntoDatos()}">	
		<rule if-outcome="ok">
			<end-conversation before-redirect="true"/>  		
			<redirect view-id="/modEnsayo/ensayo_extraccion/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarConjuntoDatos/verDetallesConjuntoDatos/verDetallesConjuntoDatos.gehos">
				<param name="idConjuntoDatos" value="#{crearConjuntoDatosVertical.conjuntoDatos.id}" />				
				<param name="from" value="crear" />
			</redirect>
		</rule>
	</navigation>
	
	<navigation from-action="cancelar">	
		
			<end-conversation before-redirect="true"/>  		
			<redirect view-id="/modEnsayo/ensayo_extraccion/modules/#{activeModule.activeSubModulePhysicalName()}/gestionarConjuntoDatos/buscarConjuntoDatos/buscarConjuntoDatos.gehos">
			
			
			</redirect>
		
	</navigation>
	
	
	
	
</page>