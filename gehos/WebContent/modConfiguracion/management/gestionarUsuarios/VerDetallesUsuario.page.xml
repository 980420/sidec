<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">

	<param name="usuarioId" value="#{usuarioVerDetallesControlador.id}" />
	<param name="order" />
	<param name="firstResult" />
	<param name="critB1" />
	<param name="critB2" />
	<param name="critB3" />
	<param name="critB4" />
	<param name="busquedaTipo" />
	<param name="open" />
	<param name="from" value="#{usuarioVerDetallesControlador.from}" />

	<navigation from-action="#{usuarioVerDetallesControlador.eliminar()}">
		<rule if-outcome="listar">		
			<redirect
				view-id="/modConfiguracion/management/gestionarUsuarios/BuscarUsuario.xhtml">
				<param name="order" value="#{order}" />
				<param name="firstResult" value="#{firstResult}" />
				<param name="critB1" value="#{critB1}" />
				<param name="critB2" value="#{critB2}" />
				<param name="critB3" value="#{critB3}" />
				<param name="critB4" value="#{critB4}" />
				<param name="busquedaTipo" value="#{busquedaTipo}" />
				<param name="open" value="#{open}" />
			</redirect>
			<end-conversation before-redirect="true" />
		</rule>
		
	</navigation>

	<navigation from-action="modificar">
		<redirect
			view-id="/modConfiguracion/management/gestionarUsuarios/ModificarUsuario.xhtml">
			<param name="usuarioId" value="#{usuarioVerDetallesControlador.usuario_conf.id}" />
			<param name="selectedTab" value="#{usuarioVerDetallesControlador.selectedTab}" />
			<param name="order" value="#{order}" />
			<param name="firstResult" value="#{firstResult}" />
			<param name="critB1" value="#{critB1}" />
			<param name="critB2" value="#{critB2}" />
			<param name="critB3" value="#{critB3}" />
			<param name="critB4" value="#{critB4}" />
			<param name="busquedaTipo" value="#{busquedaTipo}" />
			<param name="open" value="#{open}" />
		</redirect>
		<end-conversation before-redirect="true" />
	</navigation>

	<navigation from-action="salir">
		<redirect
			view-id="/modConfiguracion/management/gestionarUsuarios/BuscarUsuario.xhtml">
			<param name="order" value="#{order}" />
			<param name="firstResult" value="#{firstResult}" />
			<param name="critB1" value="#{critB1}" />
			<param name="critB2" value="#{critB2}" />
			<param name="critB3" value="#{critB3}" />
			<param name="critB4" value="#{critB4}" />
			<param name="busquedaTipo" value="#{busquedaTipo}" />
			<param name="open" value="#{open}" />
		</redirect>
		<end-conversation before-redirect="true" />
	</navigation>

</page>