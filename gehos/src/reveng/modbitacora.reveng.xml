<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-schema="seguridad" match-name="user" />
	<table-filter match-schema="bitacora" match-name="traza_accion" />
	<table-filter match-schema="bitacora"
		match-name="traza_atributo_modificado" />
	<table-filter match-schema="bitacora"
		match-name="traza_modulo_accedido" />
	<table-filter match-schema="bitacora" match-name="traza_session" />
	<table-filter match-name="modulo" match-schema="comun"></table-filter>
</hibernate-reverse-engineering>