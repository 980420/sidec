<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">

	<begin-conversation flush-mode="manual" join="true" />

	<param name="userId" value="#{medicoDetail_Controlador.id}" />
	<param name="from" value="#{medicoDetail_Controlador.from}" />
	<param name="firstResult" />
	<param name="order" />
	<param name="avz" />
	<param name="open" />
	<param name="critB1" />
	<param name="critB2" />
	<param name="critB3" />
	<param name="critB4" />
	<param name="critB5" />


	<navigation from-action="modificar">
		<rule>
			<redirect
				view-id="/modConfiguracion/management/gestionar_medicos/modificar_medico.xhtml">
				<param name="userId" value="#{medicoDetail_Controlador.id}" />
				<param name="firstResult" value="#{firstResult}" />
				<param name="order" value="#{order}" />
				<param name="avz" value="#{avz}" />
				<param name="open" value="#{open}" />
				<param name="critB1" value="#{critB1}" />
				<param name="critB2" value="#{critB2}" />
				<param name="critB3" value="#{critB3}" />
				<param name="critB4" value="#{critB4}" />
				<param name="critB5" value="#{critB5}" />
			</redirect>
			<end-conversation before-redirect="true" />
		</rule>
	</navigation>

	<navigation from-action="salir">
		<rule>
			<redirect
				view-id="/modConfiguracion/management/gestionar_medicos/medicoList.xhtml">
				<param name="firstResult" value="#{firstResult}" />
				<param name="order" value="#{order}" />
				<param name="avz" value="#{avz}" />
				<param name="open" value="#{open}" />
				<param name="critB1" value="#{critB1}" />
				<param name="critB2" value="#{critB2}" />
				<param name="critB3" value="#{critB3}" />
				<param name="critB4" value="#{critB4}" />
				<param name="critB5" value="#{critB5}" />
			</redirect>
			<end-conversation before-redirect="true" />
		</rule>
	</navigation>

	<navigation from-action="#{medicoDetail_Controlador.eliminar()}">
		<rule if-outcome="eliminar">
			<redirect
				view-id="/modConfiguracion/management/gestionar_medicos/medicoList.xhtml">
				<param name="firstResult" value="#{firstResult}" />
				<param name="order" value="#{order}" />
				<param name="avz" value="#{avz}" />
				<param name="open" value="#{open}" />
				<param name="critB1" value="#{critB1}" />
				<param name="critB2" value="#{critB2}" />
				<param name="critB3" value="#{critB3}" />
				<param name="critB4" value="#{critB4}" />
				<param name="critB5" value="#{critB5}" />
			</redirect>
			<end-conversation before-redirect="true" />
		</rule>
	</navigation>

</page>
