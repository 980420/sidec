<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd">

	<begin-conversation flush-mode="manual" join="true" />
	<action execute="#{cronogramasVisualizarCutomList_ensayo.refresh()}" />
	<navigation evaluate="#{seguridadEstudio.CanSeeThisFunctionalityNow()}">
		<rule if-outcome="not-selected">
			<redirect
				view-id="/modEnsayo/codebase/seleccionar_estudio/seleccionarEstudio.xhtml">
				<!-- Colocal el valor la direccion a la que se debe redireccionar al 
					seleccionar el estudio -->
				<param name="direccion"
					value="/modEnsayo/ensayo_disenno/modules/#{activeModule.activeSubModulePhysicalName()}/aprobarDiseno/aprobarDiseno.xhtml" />
			</redirect>
		</rule>
		<rule if-outcome="not-authorized">
			<redirect view-id="/error.xhtml">
				<message>#{messanger.msg_accesoDenegado_modEns}</message>
			</redirect>
		</rule>
	</navigation>
</page>